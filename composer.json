{
    "name": "xinningsu/flysystem-baidu-bos",
    "description": "Flysystem adapter for Baidu BOS",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Su",
            "email": "deng5765@gmail.com"
        }
    ],
    "repositories": {
