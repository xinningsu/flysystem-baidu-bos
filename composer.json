        }
    },
    "require": {
        "php": ">=5.6",
        "league/flysystem": "^1.0",
        "xinningsu/baidu-bos": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0",
        "squizlabs/php_codesniffer": "^3.0",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Sulao\\Flysystem\\BaiduBos\\": "src/"
        }
